{
  "name": "cadre-router",
  "version": "0.1.0",
  "sources": [
    { "dir": "src", "subdirs": true },
    { "dir": "examples", "subdirs": true },
    { "dir": "tests", "subdirs": true }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".js",
  "bs-dependencies": [],
  "bs-dev-dependencies": [],
  "bsc-flags": ["-open Belt"],
  "warnings": {
    "error": "+101"
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "reanalyze": {
    "analysis": ["dce", "exception"]
  }
}
